{
  "name": "eot_nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run": "npm run typeorm migration:run -- -d src/orm.config.ts",
    "typeorm:generate": "npm run typeorm -- -d src/orm.config.ts migration:generate src/migrations/$npm_config_name",
    "typeorm:create": "npm run typeorm -- migration:create src/migrations/$npm_config_name",
    "typeorm:revert": "npm run typeorm -- -d src/orm.config.ts migration:revert"
  },
  "dependencies": {
    "@adminjs/express": "5.0.1",
    "@adminjs/nestjs": "5.0.1",
    "@adminjs/typeorm": "4.0.0",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/typeorm": "9.0.1",
    "adminjs": "6.7.2",
    "bcrypt": "5.1.0",
    "connect-typeorm": "2.0.0",
    "express-formidable": "1.2.0",
    "express-session": "1.17.3",
    "passport": "0.6.0",
    "passport-local": "1.0.0",
    "pg": "8.8.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.8.0",
    "typeorm": "0.3.11"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.7",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.2.1",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.15",
    "@types/express-session": "1.17.5",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/passport": "1.0.11",
    "@types/passport-local": "1.0.34",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "eslint": "8.30.0",
    "jest": "29.3.1",
    "source-map-support": "0.5.21",
    "standard": "17.0.0",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}